/**
 * @param {number} n
 * @return {string[]}
 */
var generateParenthesis = function(n) {
    var result = [];
    
    function generate(l,r,s)
    {
        if(l == n && r == n)
        {
            result.push(s);
            return;
        }
        if(l < n)
        {
            generate(l + 1, r,s+'(');
        }
        if(l > r)
        {
            generate(l,r + 1,s+')');
        }
}
    generate(0 , 0 ,'');
    return result;
};
