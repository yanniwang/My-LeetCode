/**
 * @param {number} n
 * @return {string[]}
 */
var generateParenthesis = function(n) {
    var result = [];
    
    function generate(l,r,s)
    {
        if(l==0 && r==0)
        {
            result.push(s);
            return;
        }
        if(l > 0)
        {
            generate(l-1,r,s+'(');
        }
        if(l>0 && l<r)
        {
            generate(l,r-1,s+')');
        }
}
    generate('' , n /2 , n/2);
    return result;
};

